{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"eslint-config-prettier"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"root": true,
	"rules": {
		"@typescript-eslint/ban-types": "error",
		"@typescript-eslint/no-unused-vars": "error"
	}
}
